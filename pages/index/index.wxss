/**index.wxss**/

.searchBlock{
  display: flex;
  align-items: center;
  background-color: rgb(53, 164, 255);
  padding-top: 15rpx;
  padding-bottom: 15rpx;
}
.searchInput{
  position: relative;
  width: 90%;
  margin: 0 auto;
  /* height: 68rpx; */
  -webkit-overflow-scrolling: touch;
  background: #ffff;
  border-radius: 32rpx;
  padding-left: 10rpx;
  box-sizing: border-box;
}
.tbody{
  position: relative;
  background-color: rgb(194, 194, 194);
  width:100%;
  padding-top:10rpx;
  height: 1500rpx;
}
.text_container{
  border-radius: 20rpx;
  background-color: rgb(255, 255, 255);
  margin-left: 10rpx;
  margin-right:10rpx;
  /* height: 500rpx; */
}

